{
  "javaClass": "com.huawei.sdm.page.extend.mobile.container.MobilePage",
  "title": "home_Mateline.bullletin",
  "fullScreen": true,
  "includes": [
    {
      "javaClass": "com.huawei.sdm.page.extend.mobile.include.MCss",
      "code": "div[spl-id=\"home_bulletin_header\"]{\n    background-color: #2b2e59 !important;\n}\ndiv[spl-id=\"home_bulletin_listview\"] .bulletin_time{\n   font-size: 12px;\n}\n.bullentin_container{\n    display: flex;\n    justify-content: space-between;\n\n}\n.bulletin_from{\n    margin-left: 10px;\n}\n.bulletin_icon{\n    width: 25px;\n    height: 25px;\n    vertical-align: text-bottom;\n}\n.bulletin_text{\n    white-space: normal;\n    display:none;\n    padding: 10px;\n}\n.bulletin_header{\n    width: 70%;\n    text-overflow: ellipsis;\n    overflow: hidden;\n    white-space: nowrap;\n}"
    },
    {
      "javaClass": "com.huawei.sdm.page.extend.mobile.include.MJavaScript",
      "code": "Nf.PageReady(function(){\n    C(\"home_bulletin_listview\").on(\"dataLoaded\", function (data) {\n    \t\t$(\".bulletin_text>span\").html(Nf.res(\"home_Mateline.content\"));\n    })\n    $(\"div[spl-id='home_bulletin_listview']\").on(\"click\",\".list ion-item\",function(e){\n            e.stopPropagation();\n            $(this).find(\".bulletin_text\").slideToggle();\n    });\n})"
    },
    {
      "javaClass": "com.huawei.sdm.page.extend.mobile.include.MI18n",
      "bundle": "home_Mateline"
    }
  ],
  "body": [
    {
      "javaClass": "com.huawei.sdm.page.extend.mobile.list.MListView",
      "id": "home_bulletin_listview",
      "serviceId": "mobilesocial_notice_message_pull_getList",
      "itemIdentifyField": "id",
      "pageSize": 10,
      "ignoreItemPageHistory": false,
      "buttons": [],
      "customRowTemplate": "<div>\n    <div class=\"bullentin_container\">\n        <div class=\"bulletin_header\">\n            <img src=\"data:image/png;base64,\" class=\"bulletin_icon\">\n            <span class=\"bulletin_from\">{{row.send_from}}: </span>\n            <span>{{row.title}}</span>\n        </div>\n        <span class=\"bulletin_time\">{{row.send_time}}<span>\n    </div>\n    <div class=\"bulletin_text\"><span> </span> {{row.message_data}}</div>\n</div>",
      "canUseWhenOffline": true,
      "supportMultipleSelect": false
    }
  ],
  "header": {
